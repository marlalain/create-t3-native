{
  "name": "create-t3-native",
  "version": "0.1.0",
  "description": "Quickest way to start a new native app with full stack typesafety",
  "main": "index.js",
  "repository": "https://github.com/minsk-dev/create-t3-native.git",
  "author": "Alex <⚧@❤.gg>",
  "license": "BSD-3-Clause",
  "private": false,
  "exports": "./dist/index.js",
  "bin": {
    "create-t3-native": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm --watch --clean --onSuccess \"node dist/index.mjs\"",
    "build": "tsup src/index.ts --format esm --clean --sourcemap --minify --metafile"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^9.4.0",
    "fs-extra": "^10.1.0",
    "inquirer": "^9.1.1",
    "shelljs": "^0.8.5",
    "sort-package-json": "^1.57.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.1",
    "@types/node": "^18.7.15",
    "@types/fs-extra": "^9.0.13",
    "@types/shelljs": "^0.8.11",
    "type-fest": "^2.19.0"
  }
}
